  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "N5_DB": "Data Source=.;Initial Catalog=n5_db;User Id=sa;Password=123456;MultipleActiveResultSets=False;TrustServerCertificate=True;"
  },
  "Kafka": {
    "BootstrapServers": "yhttp://localhost:9092",
    "GroupId": "your_group_id",
    "AutoOffsetReset": "earliest" // or "latest" depending on your requirements
  }
}
